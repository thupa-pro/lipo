  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser npm run build",
    
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    
    "test": "vitest",
    "test:unit": "vitest run --config vitest.config.unit.js",
    "test:integration": "vitest run --config vitest.config.integration.js",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "prisma migrate dev --schema=./prisma/schema.test.prisma",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:seed:test": "NODE_ENV=test prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db push",
    "supabase:types": "supabase gen types typescript --local",
    
    "docker:build": "docker build -t loconomy:latest .",
    "docker:run": "docker run -p 3000:3000 loconomy:latest",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postbuild": "next-sitemap",
    
    "health-check": "node scripts/health-check.js",
    "performance-test": "node scripts/performance-test.js",
    "security-scan": "npm audit --audit-level=moderate",
    "bundle-analyze": "npm run analyze && open .next/analyze/client.html",
    
    "ai:embed": "node scripts/ai-embed.js",
    "ai:train": "node scripts/ai-train.js",
    "ai:migrate": "node scripts/ai-migrate-vectors.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:unit"
    }
  },
